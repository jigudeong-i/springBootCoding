<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.boot.example.domain.StudentMapper">
    <resultMap id="studentResult" type="com.boot.example.domain.Student">    <!-- student에 있는 필드들임을 표기 -->
        <id column="student_id" property="studentId"/>       <!-- 필드명, 컬럼명이 표기가 달라서 하는거임 -->
        <result column="sd_num" property="studentNumber"/>
        <result column="sd_name" property="studentName"/>
        <result column="sd_id" property="studentId"/>
        <result column="sd_passwd" property="studentPassword"/>
        <result column="s_num" property="subjectNumber"/>
        <result column="sd_birth" property="studentBirth"/>
        <result column="sd_phone" property="studentPhone"/>
        <result column="sd_address" property="studentAddress"/>
        <result column="sd_email" property="studentEmail"/>
        <result column="sd_date" property="studentDate"/>
    </resultMap>

    <select id="studentList" resultMap="studentResult">
        SELECT
        student_id, sd_num, sd_name, sd_id, sd_birth, sd_phone, sd_address, sd_email, sd_date
        FROM student
        ORDER BY student_id desc
    </select>
</mapper>